#
# Version: 0.1.3
#
# If you edit this, do not forget to uncomment any lines that you change.
# The pound(#) symbol may be used anywhere for comments.
#
# A list of keys is in /usr/include/X11/keysym.h and in
# /usr/include/X11/keysymdef.h 
# The XK_ is not needed. 
#
# List of modifier (on my keyboard): 
#   Control, Shift, Mod1 (Alt), Mod2 (NumLock), 
#   Mod3 (CapsLock), Mod4, Mod5 (Scroll). 
#
# Another way to specifie a key is to use 'xev' and set the 
# keycode with c:nnn or the modifier with m:nnn where nnn is 
# the keycode or the state returned by xev 
#
# This file is created by xbindkey_config 
# The structure is : 
# # Remark 
# "command" 
# m:xxx + c:xxx 
# Shift+... 




keystate_numlock = enable
keystate_scrolllock = enable
keystate_capslock = enable



"xbindkeys_show"
   control+shift+alt + k

# kill active window:
"xdotool click 1; sleep .2;  wmctrl -c :ACTIVE:"
   Mod4 + shift + Delete

"systemsettings"
   control+shift + p

"kmenuedit"
    m:0x4d + c:45
    Control+Shift+Alt+Mod4 + k 

# jumpapp
"code"
   Mod4 + alt + C

"jumpapp code"
   Mod4 + c

"firefox"
   Mod4 + alt + f

"jumpapp firefox"
   Mod4 + f

"google-chrome-stable"
   Mod4 + alt + h

"jumpapp google-chrome-stable"
   Mod4 + h

"terminator"
   Mod4 + alt + t

"jumpapp terminator"
   Mod4 + t

"konsole"
    m:0x48 + c:45
    Alt+Mod4 + k 

"jumpapp konsole"
   Mod4 + k

#jump dolphin
"jumpapp dolphin"
    m:0x40 + c:26
    Mod4 + e 

#Open dolphin
"dolphin"
    m:0x48 + c:26
    Alt+Mod4 + e 

# increase window height
"WINDOW_HEIGHT=$((`xwininfo -id \`xdotool getactivewindow\`|grep Height|cut -d: -f2`+50)); wmctrl -r :ACTIVE: -b remove,maximized_vert; wmctrl -r :ACTIVE: -e 0,-1,-1,-1,$WINDOW_HEIGHT"
   Mod4 + control + shift + j

